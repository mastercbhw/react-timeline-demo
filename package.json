{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@umijs/fabric": "2.0.11",
    "@umijs/hooks": "^1.9.3",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.20",
    "antd": "3.26.18",
    "classnames": "^2.2.6",
    "faker": "^5.1.0",
    "interactjs": "^1.10.0",
    "lint-staged": "^10.0.7",
    "moment": "^2.28.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "randomcolor": "^0.6.2",
    "rc-chart-slider": "^0.0.3",
    "react": "^16.12.0",
    "react-calendar-timeline": "^0.27.0",
    "react-dom": "^16.12.0",
    "react-jsgantt": "^2.4.3",
    "swiper": "^6.3.2",
    "umi": "^3.2.20",
    "uuid": "^3.4.0",
    "yorkie": "^2.0.0"
  }
}
